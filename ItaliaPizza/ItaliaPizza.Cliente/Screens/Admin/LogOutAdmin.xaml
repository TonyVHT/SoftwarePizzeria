<Window x:Class="ItaliaPizza.Cliente.Screens.Admin.LogOutAdmin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ItaliaPizza.Cliente.Screens"
        xmlns:controls="clr-namespace:ItaliaPizza.Cliente.UserControls"
        mc:Ignorable="d"
        Title="LogOutAdmin" Height="450" Width="800">
    <Grid>

        <Grid>
            <controls:UCAdmin></controls:UCAdmin>
        </Grid>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="82*"/>
                <RowDefinition Height="54*"/>
                <RowDefinition Height="81*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="80*"/>
                <ColumnDefinition Width="50*"/>
                <ColumnDefinition Width="80*"/>
            </Grid.ColumnDefinitions>
            <Button Style="{StaticResource buttonsStyle}" x:Name="btnCerrarSesion" Grid.Column="1" Grid.Row="1" Margin="10" Click="BtnCerrarSesion_Click">
                Cerrar Sesión
            </Button>

        </Grid>
    </Grid>
</Window>
